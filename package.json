{
  "name": "vite_react_shadcn_ts",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:dev": "vite build --mode development",
    "build:safe": "vite build --emptyOutDir --minify=false",
    "build:ci": "vite build --config vite.config.ci.ts",
    "build:analyze": "vite build --mode analyze",
    "lint": "eslint \"./{src,scripts,test}/**/*.{ts,tsx,js}\" --max-warnings=0",
    "lint:fix": "eslint \"./{src,scripts,test}/**/*.{ts,tsx,js}\" --fix",
    "lint:strict": "eslint \"./{src,scripts,test}/**/*.{ts,tsx,js}\" --max-warnings=0 --no-error-on-unmatched-pattern",
    "lint:all": "bun run lint && bun run format:check",
    "format": "prettier --write \"./**/*.{ts,tsx,js,jsx,json,css,scss,md}\"",
    "format:check": "prettier --check \"./**/*.{ts,tsx,js,jsx,json,css,scss,md}\"",
    "format:staged": "pretty-quick --staged",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ci": "vitest run --coverage --reporter=junit --outputFile=./coverage/junit.xml",
    "check-env": "bun scripts/check-env-vars.js",
    "check": "bun scripts/fix-lint.js",
    "fix": "bun run format && bun run lint:fix && bun run type-check",
    "verify-env": "bun scripts/verify-env.js",
    "type-check": "tsc --noEmit",
    "clean": "bun scripts/clean.js",
    "deploy:staging": "bun scripts/deploy.js staging",
    "deploy:production": "bun scripts/deploy.js production",
    "ci:status": "bun scripts/check-ci-status.js",
    "ci:verify": "bun run verify-env && bun run type-check && bun run lint:all && bun run test:ci",
    "prepare": "husky install",
    "ts:check-config": "node scripts/check-typescript-config.js",
    "ts:fix-react-imports": "node scripts/fix-react-imports.js",
    "ts:find-missing-types": "node scripts/find-missing-return-types.js",
    "ts:fix-all": "node scripts/run-typescript-fixes.js",
    "ts:commit-fixes": "node scripts/commit-typescript-fixes.js"
  },
